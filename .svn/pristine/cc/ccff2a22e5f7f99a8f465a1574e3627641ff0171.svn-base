<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EatListActivity" >



    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:background="#ff33b5e5"
        android:layout_marginTop="0dp"
        android:text="  序号1        姓名                 时间                  份数       金额(元)    " />
    <com.scme.order.view.XListView
        android:id="@+id/lvEats"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"/>
    <RelativeLayout

        android:layout_width="fill_parent"
        android:layout_height="30dp"
        android:background="#ff33b5e5"
        android:layout_alignParentBottom="true"
        >

    <RelativeLayout

        android:layout_width="fill_parent"
        android:layout_height="30dp"
        android:background="#ff33b5e5"

        >
        <Button
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="1dp"
            android:layout_marginLeft="5dp"
            android:textColor="#000000"
            android:onClick="eats_pages_sumbit_Event"
            android:text="转到页码"
            android:background="@drawable/button_zise"
            />
        <TextView
            android:id="@+id/TolPage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="1dp"
            android:layout_marginLeft="95dp"
            android:textColor="#000000"
            android:textSize="17dp"
            android:background="#ff33b5e5"

            />
        <TextView
            android:id="@+id/NowPage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="1dp"
            android:layout_marginLeft="230dp"
            android:textColor="#000000"
            android:textSize="17dp"
            android:background="#ff33b5e5"

            />

    </RelativeLayout>

    </RelativeLayout>
</RelativeLayout>
